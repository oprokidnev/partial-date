{
    "name":              "oprokidnev/partial-date",
    "description":       "Partial date with compare support",
    "type":              "library",
    "license":           "MIT",
    "homepage":          "https://github.com/oprokidnev/partial-date",
    "keywords":          [
        "datetime"
    ],
    "authors": [
        {
            "name":     "Andrey Oprokidnev",
            "email":    "oprokidnev@gmail.com"
        }
    ],
    "require": {
        "php":                       "^7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit":              "^5.6.4",
        "phpunit/phpunit-mock-objects": "^3.4.1",
        "nikic/php-parser":             "^3.0.4",
        "symfony/var-dumper": "^3.3"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Oprokidnev\\PartialDate\\": "src/PartialDate"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oprokidnev\\Tests\\PartialDate\\": "tests/PartialDate"
        }
    },
    "scripts":{
        "tests":"if ! [ -f ./vendor/bin/phpunit ]; then echo \"Run composer install\"; else php ./vendor/bin/phpunit;  fi;"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
